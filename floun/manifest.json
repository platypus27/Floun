{
  "manifest_version": 3,
  "name": "Floun",
  "version": "1.0",
  "description": "Quantum Resistant Protocol Analyzer",
  "permissions": ["activeTab", "webRequest", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "src/index.html",
    "default_icon": {
      "192": "public/logo192.png",
      "512": "public/logo512.png"
    }
  },
  "icons": {
    "192": "public/logo192.png",
    "512": "public/logo512.png"
  }
}